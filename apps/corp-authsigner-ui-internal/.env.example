# Internal App Environment Variables

# App Configuration
VITE_APP_NAME="Corp Auth Signer - Internal"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Internal corporate authentication signing application"

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_API_TIMEOUT=30000
VITE_INTERNAL_API_BASE_URL=http://localhost:8000/internal/api

# Authentication
VITE_AUTH_PROVIDER=internal
VITE_AUTH_DOMAIN=internal.company.com
VITE_AUTH_CLIENT_ID=internal-client-id
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ENABLE_DEBUGGING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_BOUNDARY=true
VITE_ENABLE_HOT_RELOAD=true

# UI Configuration  
VITE_THEME=corporate
VITE_DEFAULT_LANGUAGE=en
VITE_TIMEZONE=America/New_York

# Analytics
VITE_ANALYTICS_ID=GA-INTERNAL-XXXXXXXXX
VITE_ENABLE_ANALYTICS=false

# External Services
VITE_SIGNATURE_SERVICE_URL=http://localhost:8001
VITE_DOCUMENT_SERVICE_URL=http://localhost:8002

# Security
VITE_CSP_NONCE=your-csp-nonce-here
VITE_ALLOWED_ORIGINS=http://localhost:3001,https://internal.company.com